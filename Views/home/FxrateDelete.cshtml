<script>
var currentTable = @Html.Raw(ConvertToJson(fxrateDelete.ToClientVar()));
ew.deepAssign(ew.vars, { tables: { fxrate: currentTable } });
var currentPageID = ew.PAGE_ID = "delete";
var currentForm;
var ffxratedelete;
loadjs.ready(["wrapper", "head"], function () {
    let $ = jQuery;
    let fields = currentTable.fields;

    // Form object
    let form = new ew.FormBuilder()
        .setId("ffxratedelete")
        .setPageId("delete")
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
<script>
loadjs.ready("head", function() {
    // Write your client script here, no need to add script tags.
});
</script>
@fxrateDelete.ShowPageHeader()
@fxrateDelete.ShowMessages()
<form name="ffxratedelete" id="ffxratedelete" class="ew-form ew-delete-form" action="@AppPath(CurrentPageName())" method="post" novalidate autocomplete="on">
@if (CurrentPage.CheckToken) {
    <input type="hidden" name="@Config.TokenNameKey" value="@Config.TokenName"><!-- CSRF token name -->
    @Html.AntiForgeryToken()
}
<input type="hidden" name="action" id="action" value="delete">
@foreach (string key in fxrateDelete.RecordKeys) {
<input type="hidden" name="key_m[]" value="@key">
}
<div class="card ew-card ew-grid @(fxrateDelete.TableGridClass)">
<div class="card-body ew-grid-middle-panel @(fxrateDelete.TableContainerClass)" style="@(fxrateDelete.TableContainerStyle)">
<table class="@(fxrateDelete.TableClass)">
    <thead>
    <tr class="ew-table-header">
@if (fxrateDelete.id.Visible) { // fxrateDelete.id.Visible (begin)
        <th class="@Html.Raw(fxrateDelete.id.HeaderCellClass)"><span id="elh_fxrate_id" class="fxrate_id">@Html.Raw(fxrateDelete.id.Caption)</span></th>
} @* fxrateDelete.id.Visible (end) *@
@if (fxrateDelete.sourceCurrency.Visible) { // fxrateDelete.sourceCurrency.Visible (begin)
        <th class="@Html.Raw(fxrateDelete.sourceCurrency.HeaderCellClass)"><span id="elh_fxrate_sourceCurrency" class="fxrate_sourceCurrency">@Html.Raw(fxrateDelete.sourceCurrency.Caption)</span></th>
} @* fxrateDelete.sourceCurrency.Visible (end) *@
@if (fxrateDelete.targetCurrency.Visible) { // fxrateDelete.targetCurrency.Visible (begin)
        <th class="@Html.Raw(fxrateDelete.targetCurrency.HeaderCellClass)"><span id="elh_fxrate_targetCurrency" class="fxrate_targetCurrency">@Html.Raw(fxrateDelete.targetCurrency.Caption)</span></th>
