@{
    // View
    View = this;

    // Check action result
    if (fxrateDelete.ActionResult is EmptyResult) { // Terminate page
        return;
    }

    // Layout
    if (!fxrateDelete.UseLayout) // UseLayout = false // DN
        Layout = "";
