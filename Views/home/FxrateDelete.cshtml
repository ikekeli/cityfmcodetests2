} @* fxrateDelete.rate.Visible (end) *@
    </tr>
    </thead>
    <tbody>
@{ fxrateDelete.RecordCount = 0; }
@while (fxrateDelete.Recordset != null && await fxrateDelete.Recordset.ReadAsync()) { // DN
    fxrateDelete.RecordCount++;
    fxrateDelete.RowCount++;

    // Set row properties
    fxrateDelete.ResetAttributes();
    fxrateDelete.RowType = RowType.View; // View

    // Get the field contents
    await fxrateDelete.LoadRowValues(fxrateDelete.Recordset);

    // Render row
    await fxrateDelete.RenderRow();
    <tr @Html.Raw(fxrateDelete.RowAttributes)>
@if (fxrateDelete.id.Visible) { // fxrateDelete.id.Visible (begin)
        <td @Html.Raw(fxrateDelete.id.CellAttributes)>
<span id="el@(fxrateDelete.RowCount)_fxrate_id" class="el_fxrate_id">
<span @Html.Raw(fxrateDelete.id.ViewAttributes)>
@Html.Raw(fxrateDelete.id.GetViewValue())</span>
</span></td>
} @* fxrateDelete.id.Visible (end) *@
@if (fxrateDelete.sourceCurrency.Visible) { // fxrateDelete.sourceCurrency.Visible (begin)
        <td @Html.Raw(fxrateDelete.sourceCurrency.CellAttributes)>
<span id="el@(fxrateDelete.RowCount)_fxrate_sourceCurrency" class="el_fxrate_sourceCurrency">
<span @Html.Raw(fxrateDelete.sourceCurrency.ViewAttributes)>
@Html.Raw(fxrateDelete.sourceCurrency.GetViewValue())</span>
</span></td>
} @* fxrateDelete.sourceCurrency.Visible (end) *@
@if (fxrateDelete.targetCurrency.Visible) { // fxrateDelete.targetCurrency.Visible (begin)
        <td @Html.Raw(fxrateDelete.targetCurrency.CellAttributes)>
<span id="el@(fxrateDelete.RowCount)_fxrate_targetCurrency" class="el_fxrate_targetCurrency">
<span @Html.Raw(fxrateDelete.targetCurrency.ViewAttributes)>
@Html.Raw(fxrateDelete.targetCurrency.GetViewValue())</span>
</span></td>
} @* fxrateDelete.targetCurrency.Visible (end) *@
@if (fxrateDelete.rate.Visible) { // fxrateDelete.rate.Visible (begin)
